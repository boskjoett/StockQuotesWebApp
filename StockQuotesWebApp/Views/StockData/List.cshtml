@model StockQuotesWebApp.Models.StockDataSet

@{
    ViewData["Title"] = "Stock Data";
}

<h2>Stock Data</h2>

<div class="row">
    <div class="col-md-2">
        <b>ID</b>
    </div>
    <div class="col-md-10">
        @Html.DisplayFor(model => model.Id)
    </div>
</div>

<div class="row">
    <div class="col-md-2">
        <b>Name</b>
    </div>
    <div class="col-md-10">
        @Html.DisplayFor(model => model.Name)
    </div>
</div>

<div class="row">
    <div class="col-md-2">
        <b>Stock Exchange</b>
    </div>
    <div class="col-md-10">
        @Html.DisplayFor(model => model.StockExchange)
    </div>
</div>

<div class="row">
    <div class="col-md-2">
        <b>Description</b>
    </div>
    <div class="col-md-10">
        @Html.DisplayFor(model => model.Description)
    </div>
</div>

<br />
<br />

<table class="table">
    <thead>
        <tr>
            <th>
                Date
            </th>
            <th>
                Low
            </th>
            <th>
                High
            </th>
            <th>
                Close
            </th>
            <th>
                Traded Volume
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.TimeSeries)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Date)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Low)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.High)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Close)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TradedVolume)
                </td>
            </tr>
        }
    </tbody>
</table>
